<view><view data-event-opts="{{[['tap',[['chooseImg',[0]]]]]}}" class="upload" bindtap="__e"><button class="upload_icon iconfont" type="primary"></button><image class="img" src="{{this.imgArr[0]}}"></image><text>上传头像</text></view><view class="personal_info"><view class="person">昵称</view><input class="info_input" placeholder="请输入昵称" data-event-opts="{{[['input',[['__set_model',['$0','nickname','$event',[]],['info']]]]]}}" value="{{info.nickname}}" bindinput="__e"/><view class="person">性别</view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="radios" bindchange="__e"><radio class="radio" value="man" name="sex" checked="true">男</radio><radio class="radio" value="woman" name="sex">女</radio></radio-group><view class="person">姓名</view><input class="info_input" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['info']]]]]}}" value="{{info.name}}" bindinput="__e"/><view class="person">学号</view><input class="info_input" placeholder="请输入学号" data-event-opts="{{[['input',[['__set_model',['$0','stu_number','$event',[]],['info']]]]]}}" value="{{info.stu_number}}" bindinput="__e"/></view><view class="info"><view class="province">省份</view><picker mode="selector" range="{{provinces}}" data-event-opts="{{[['change',[['choiceProvince',['$event']]]]]}}" bindchange="__e">{{province}}</picker><view class="province">学校</view><picker mode="selector" range="{{schools}}" data-event-opts="{{[['change',[['choiceCollege',['$event']]]]]}}" bindchange="__e">{{school}}</picker></view><view data-event-opts="{{[['tap',[['chooseImg',[1]]]]]}}" class="uploadPhoto" bindtap="__e"><button class="upload_icon iconfont" type="primary" data-event-opts="{{[['tap',[['chooseImg',['$event']]]]]}}" bindtap="__e"></button><image class="img" src="{{this.photoArr[0]}}"></image><text>上传学生证照片</text></view><button class="finish" disabled="{{button_status}}" data-event-opts="{{[['tap',[['allFinsh',['$event']]]]]}}" bindtap="__e">注册</button><view class="demo"></view></view>